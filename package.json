{
  "name": "hdb-resale-data",
  "version": "1.0.0",
  "description": "mini hack to display HDB resale price",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run build-index && npm run start-server",
    "start-server": "node dist/server.js",
    "build-index": "cp public/index.html dist/public/index.html && babel public --out-dir dist/public --presets es2015",
    "build": "babel src --out-dir dist --presets es2015",
    "babel-node": "babel-node --presets es2015 --ignore='foo|bar|baz'",
    "test": "standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caalberts/hdb-resale-data.git"
  },
  "author": "Yong Jun, Jared, Albert",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/caalberts/hdb-resale-data/issues"
  },
  "homepage": "https://github.com/caalberts/hdb-resale-data#readme",
  "dependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-preset-es2015": "^6.1.18",
    "body-parser": "^1.14.1",
    "express": "^4.13.3",
    "mongoose": "^4.2.9",
    "node-fetch": "^1.3.3"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "mocha": "^2.3.4",
    "standard": "^5.4.1"
  }
}
